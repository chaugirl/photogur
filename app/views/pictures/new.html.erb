<h1>Add a picture</h1>
<%= form_for "/pictures" do  |f| %>
	<p>
		<%= f.label "artist" %><br>
		<%= f.text_field "artist" %>
	</p>
	<p>
		<%= f.label "title" %><br>
		<%= f.text_field "title" %>
	</p>
	<p>
		<%= f.label "url" %><br>
		<%= f.text_field "url" %>
	</p>
	<p>
		<%= f.submit "Save" %>
	</p>
<% end %>

<!-- using Form helpers - methods you can invoke that write HTML code for us -->
